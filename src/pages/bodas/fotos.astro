---
import { Image } from 'astro:assets'
import Layout from '../../layouts/Layout.astro'
import fotoBoda1 from '../../../public/fotosBodas/04102024-DSC000132.jpg'
---

<Layout
	title='Fotos de bodas'
	description='Las mejores fotos de bodas'
>
	<main class='grid h-screen w-screen place-content-center'>
		<section
			class='mx-auto grid w-full max-w-screen-xl auto-rows-[35rems] grid-cols-10 gap-4 border border-white p-20'
		>
			<Image
				src={fotoBoda1}
				alt='Foto boda'
			/>
		</section>
	</main>
</Layout>
